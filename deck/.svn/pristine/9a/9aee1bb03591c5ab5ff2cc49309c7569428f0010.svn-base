.siteBox{
	height: 30px;
	padding-top: 20px;
	margin-bottom: 10px;
}
.site{
	width: 30%;
	height: 30px;
	background: url(../img/site.png) no-repeat 5px;
	background-size: 14px;
	line-height: 30px;
	text-indent: 25px;
	float: left;
}
.siteName{
	width: 65%;
	height: 30px;
	border: 2px solid #1a9bd2;
	background: url(../img/dao.png) no-repeat 96%;
	background-size: 12px;
	text-align: center;
	line-height: 30px;
	margin-bottom: 10px;
	float: right;
	margin-right: 2%;
}
.siteName1{
	width: 65%;
	height: 30px;
	border: 2px solid #1a9bd2;
	background:no-repeat 96%;
	background-size: 12px;
	text-align: center;
	line-height: 30px;
	margin-bottom: 10px;
	float: right;
	margin-right: 2%;
}
.goumai{
	width: 90%;
	height: 30px;
	background: url(../img/goumai.png) no-repeat left;
	background-size: 20px;
	line-height: 30px;
	text-indent: 25px;
}
.tab{
	width: 100%;
	height: 50px;
}
.tab > p{
	width: 50%;
	height: 100%;
	text-align: center;
	line-height: 50px;
	color: #5d5d5d;
	float: left;
	position: relative;
	text-indent: -8px;
}
.tab > p > span{
	width: 16px;
	height: 16px;
	display: inline-block;
	border: 2px solid #8a8a8a;
	position: absolute;
	border-radius: 50%;
	top: 15px;
	margin-left: 8px;
}
.tab > p > span > i{
	width: 8px;
	height: 8px;
	background: #8a8a8a;
	display: inline-block;
	border-radius: 50%;
	position: absolute;
	top: 4px;
	left: 4px;
}
.tab > p.on > span{
	border-color: #47a4dc;
}
.tab > p.on > span > i{
	background: #47a4dc;
}
.favorable{
	width: 94%;
	padding: 3%;
	background: #f5f5f5;
}
.title{
	width: 100%;
	color: #5d5d5d;
	background: /* url(../img/yhh.png) */ no-repeat right;
	background-size: 20px;
	margin-bottom: 10px;
}
.title.on{
	background: url(../img/yh.png) no-repeat right;
	background-size: 20px;
}
.nr{
	color: #ff8025;
	font-size: .8em;
}
.payType{
	width: 94%;
	height: 39px;
	padding: 0 3%;
	line-height: 40px;
	border-bottom: 1px solid #e6e6e6;
	line-height: 40px;
	position: relative;
}
.payType > span{
	width: 150px;
	height: 28px;
	background: #e6f8ff url(../img/xl.png) no-repeat right;
	text-align: center;
	background-size: 28px;
	display: inline-block;
	float: right;
	margin-top: 6px;
	line-height: 28px;
	text-indent: -28px;
	color: #5d5d5d;
}
.payMony{
	width: 94%;
	height: 39px;
	padding: 0 3%;
	line-height: 40px;
	border-bottom: 1px solid #e6e6e6;
	line-height: 40px;
	position: relative;
}
.payMony > span{
	float: right;
}
.pullDown{
	width: 150px;
	position: absolute;
	top: 35px;
	right: 3%;
	display: none;
	z-index: 1;
}
.pullDown > li{
	height: 28px;
	color: #5d5d5d;
	text-align: center;
	margin-bottom: 1px;
	background: #e6f8ff;
	line-height: 28px;
}
.payNum{
	width: 94%;
	height: 40px;
	padding: 0 3%;
	line-height: 40px;
	line-height: 40px;
	position: relative;
}
.payNum > p{
	width: 150px;
	height: 28px;
	text-align: center;
	background-size: 28px;
	display: inline-block;
	float: right;
	margin-top: 6px;
	line-height: 28px;
	color: #5d5d5d;
}
.payNum > p > span{
	width: 26px;
	height: 26px;
	border: 1px solid #5d5d5d;
	color: #5d5d5d;
	text-align: center;
	line-height: 22px;
	display: inline-block;
	font-size: 1.8em;
	text-indent: 0;
	float: left;
}
.payNum > p > input{
	width: 82px;
	height: 26px;
	border: 1px solid #5d5d5d;
	float: left;
	margin: 0 5px;
	text-align: center;
}
.give{
	width: 94%;
	height: 39px;
	padding: 0 3%;
	line-height: 40px;
	border-bottom: 1px solid #e6e6e6;
	line-height: 40px;
	position: relative;
}
.give > span{
	height: 28px;
	text-align: center;
	background-size: 28px;
	display: inline-block;
	float: right;
	margin-top: 6px;
	line-height: 28px;
	color: #5d5d5d;
}
.paySum{
	height: 40px;
	line-height: 40px;
	text-align: right;
	width: 97%;
	padding-right: 3%;
}
.paySum > span{
	color: #ff5a27;
}
.payTitle{
	width: 100%;
	height: 38px;
	background: #f5f5f5 url(../img/xq.png) no-repeat 14px center;
	line-height: 38px;
	text-indent: 38px;
	font-size: .9em;
}

.balance{
	float: right;
	padding-left: 18px;
	text-align: right;
	font-size: 14px;
	margin-right: 3%;
	background: url(../img/yh.png) no-repeat left;
	background-size: 14px;
}
.balance.on{
	background: url(../img/yhh.png) no-repeat left;
	background-size: 14px;
}
.goPay{
	width: 100%;
	height: 40px;
	text-align: center;
	background: #f6f6f6;
	position: fixed;
	bottom: 0;
}
.goPay > span{
	width: 100%;
	height: 40px;
	display: inline-block;
	text-align: center;
	color: #fff;
	background: #47a4dc;
	line-height: 40px;
	
}
.maxkXl,
.maskSite{
	width: 100%;
	height: 100%;
	background: rgba(119,119,119,.3);
	position: fixed;
	top: 0;
	left: 0;
	display: none;
}
.content{
	width: 100%;
	height: 46%;
	background: #f5f5f5;
	position: absolute;
	left: 0;
	bottom: -46%;
	overflow: auto;
}
.tc{
	width: 95%;
	height: 30px;
	border-bottom: 1px #999 solid;
	background: #e8e8e8;
	text-align: right;
	line-height: 30px;
	padding-right: 5%;
}
.tc > span{
	color: #47a4dc;
}
.payTypeList,
.siteList{
	width: 100%;
	height: 80%;
	margin-top: 10px;
	overflow: auto;
}
.payTypeList > ul > li,
.siteList > li{
	width: 100%;
	text-align: center;
	line-height: 40px;
	height: 40px;
	background: #f5f5f5;
	border-bottom: 1px solid #fff;
	color: #969696;
	font-weight: bold;
}
.mask{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	background: rgba(119,119,119,.3) url(../img/wx.png) no-repeat center;
	background-size: 90%;
	display: none;
}
.yeMask{
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(119,119,119,.3);
  left: 0;
  top: 0;
  display: none;
}
.yeBox{
  width: 80%;
  height: 110px;
  top: 65%;	
  left: 10%;
  background: #fff;
  position: absolute;
  text-align: center;
  font-size: 14px;
  padding-top: 30px;
}
.yeBox > button{
  margin-top: 30px;
  width: 80px;
  height: 32px;
  border: none;
  background: #ccc;
  color: #fff;
}
.yeBox > button:nth-child(2){
  background: skyblue !important;
  margin-right: 10px;
}





