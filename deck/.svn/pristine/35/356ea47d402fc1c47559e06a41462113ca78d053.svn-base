}
/* 提示功能样式 */
.mask{
    width: 100%;
    background: rgba(0,0,0,0.6);
    position: fixed;
    left: 0;
    top: 0;
    display: none;
    overflow: hidden;
}
.mask>div{
    box-shadow: 0 0 5px #fff;
    border: 1px #ccc solid;
    position: absolute;
}
.diao{
    width: 80%;
    height: 140px;
    border-radius: 80%;
    left: 10%;
    bottom: -200px;
    background: url(../img/newly.png) no-repeat center;
    background-size: 100%;
}
.diao>.clos{
    width: 30px;
    height: 30px;
    position: absolute;
    right: 0;
    top: 0;
    display: block;
    background: url(../img/clos.png) no-repeat center;
    background-size: 100%;
}
.diao1{
    width: 16%;
    height: 36px;
    border-radius: 55%;
    left: 20%;
    bottom: -60px;
    background: url(../img/new-1.png) no-repeat center;
    background-size: 100%;
}
.diao2{
    width: 8%;
    height: 20px;
    border-radius: 55%;
    left: 8%;
    bottom: -30px;
    background: url(../img/new-1.png) no-repeat center;
    background-size: 100%;
}
@media screen and (max-width: 480px){
body{max-width:480px;}
}
@media screen and (max-width: 640px){
body{max-width:640px;}
}
@media screen and (min-width: 640px){
body{min-width:640px;}
.box{width:640px; height:auto; position:relative;}
#we{font-size:34px;}
#mf input{font-size:34px;}
#fx{font-size:24px; opacity:0.5; filter:alpha(opacity=50);}
#btn input{font-size:34px;}
}
/* 脚部 copyRight */
footer{
	position: fixed;
	bottom: 10px;
	width: 100%;
	text-align: center;
	color: rgba(0,0,0,0.3);
	font-size: 12px;
}
 
/*           安全提醒           */
.ui-pic{
	width: 178px;
	height: 178px;
	background: url(../img/th.png) no-repeat center;
	margin-top: 12%;
	border-radius: 50%;
}
.often{
	color: #666;
	width: 100%;
	text-align: center;
	font-size: 18px;
	font-weight: 500;
	margin-top: 20px;
}
.ui-txt{
	color: #999;
	width: 100%;
	text-align: center;
	font-size: 14px;
	margin-top: 40px;
}
.ui-txt>a{
	color: skyblue;
	text-decoration: underline;
}
.tel{
	text-align: center;
	width: 100%;
	margin-top: 40px;
	color: #666;
	font-size: 14px;
}
.tel>a{
	color: #43c5ea;
	text-decoration: underline;
}

.tel>a.blue{
	position: relative;
	
	cursor: pointer;
	display: inline-block;
}
.tel>a>span.num{
	position: absolute;
	top: -35px;
	left: -120%;
	width: 170px;
	height: 38px;
	text-align: center;
	line-height: 30px;
	background: url(../img/xx.png) no-repeat center;
	display: none
}
.tel>a.blue:hover>span{
	display: block;
}