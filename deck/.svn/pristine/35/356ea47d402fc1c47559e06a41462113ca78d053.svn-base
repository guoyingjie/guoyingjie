form p.yz{
	border-top:none;
}
form .xb{
	padding-left:10%; width:90%;
}
form .xb input{
	width:43.75%; height:100%; display:block; font-size:13px; font-family:"微软雅黑"; color:#ccc; padding-left:15%;
}
