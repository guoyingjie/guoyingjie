@charset "utf-8";
/* CSS Document */
/*--------------公有样式-----------------*/
body{
	background:#f0f0f0;
}
.box{
	width:100%; height:auto; position:relative;
}
/*--------------公有头部样式-----------------*/
.header{
	width:100%; height:9.2%; background:#fff; top:0; position:absolute; text-align:center; border-bottom:1px #999999 solid;
}
.header p{
	width:100%; height:18px; text-align:center; margin-top:3.5%; font-family:"微软雅黑"; position:relative;
}
.header span{
	display:block; float:left;
}
.header .pic{
	margin-left:5.3%;
}
.header .dl{
	font-size:18px; text-align:center; width:68.8%;
}
.header .zc{
	font-size:13px; color:#666; margin-top:1.8%;
}
/*----------------主页样式-------------------*/
#we,#mf,#fx,#btn{
	position:absolute; font-family:"Arial";
}
#we{
	font-size:28px; font-weight:bold; color:#FFF; left:7.5%; top:12.1%; opacity:0.6; filter:alpha(opacity=60);
}
#mf{
	width:44.8%; height:31.2%; border-radius:50%; left:27.6%; top:30%; -webkit-animation-fill-mode: both; animation-fill-mode: both; -webkit-animation: load7 4s infinite ease-in; animation: load7 4s infinite ease-in;
}
@-webkit-keyframes load7 {
  0%,
  80%,
  100% {
    box-shadow: 0 0px 0 1.5 rgba(102,141,52,0.35);
  }
  40% {
    box-shadow: 0 0px 0 3em rgba(102,141,52,0.15);
  }
}
@keyframes load7 {
  0%,
  80%,
  100% {
    box-shadow: 0 0px 0 1.5 rgba(102,141,52,0.35);
  }
  40% {
    box-shadow: 0 0px 0 3em rgba(102,141,52,0.15);
  }
}
.sybtn{
	width:100%; height:100%; background:rgba(182,169,19,0.5); display:block; border-radius:50%;
}
.sybtn input{
	width:100%; height:100%; background:none; display:block; text-align:center; font-size:18px; color:#fff; font-family:"微软雅黑"; border:none; border-radius:50%; letter-spacing:3px;
}
#fx{
	font-family:"黑体"; font-size:14px; color:#FFF; top:73.3%; display:block; width:100%; text-align:center; opacity:0.5; filter:alpha(opacity=50);
}
#btn{
	width:86%; height:9%; left:7%; top:85.8%;
}
#btn input{
	width:45.5%; height:100%; display:block; font-size:16px; color:rgba(255,255,255,0.7); text-align:center; border:1px solid rgba(255,255,255,0.3); background:none; font-family:"微软雅黑";
}
#btn .btn1{
	float:left;
}
#btn .btn2{
	float:right;
}
/*-----------------公有底部样式--------------------*/
#cr{
	width:100%; font-size:12px; color:#999999; top:93%; text-align:center; position:absolute;
}
/*----------------登陆页样式-------------------*/
.banner{
	width:100%; height:13.1%; position:absolute; top:8.3%; overflow:hidden;
}
.banner .main{
	width:200%; height:13.1%; position:absolute; top:8.3%;
}
.banner .main li{
	float:left; width:50%;
}
.banner .ctrl{
	width:8%; height:10%; bacckground:#000; position:absolute; left:47.6%; top:85%;
}
.banner .ctrl li{
	width:20%;height:92%; border:1px solid #FFF; float:left; border-radius:50%; margin-right:25%;
}
.banner .ctrl li.cm{
	margin-right:0;
}
.banner .ctrl li.on{
	background:#fff;
}
.banner .gb{
	width:7.1%; height:38.3%; background:#000 url(http://oss.solarsys.cn/0001/mobile_img/gb-x.png) no-repeat center; opacity:0.2; filter:alpha(opacity=20); position:absolute; left:87.1%; top:33.3%; border-radius:50%;
}
.dlform{
	width:100%; height:37.1%; position:absolute; top:25%;
}
form p{
	width:100%; height:25.7%; background:#f0f0f0; border-top:1px solid #ccc; line-height:50%; position:relative;
}
form p.x{
	border-bottom:1px solid #ccc;
}
form p input{
	width:70.4%; height:100%; border:none; background:none; padding-left:3%; color:#666; float:left;
}

form p input#phonecode{
	width:50%; 
}

form p img{
	display:block; float:left; margin-top:3%;
}
form p img.tu3{
	margin-top:5.5%; cursor:pointer;
}
form p img.tu1{
	margin-top:3.5%;
}
form p img.tu1,form p img.tu2{
	margin-left:4%;
}
form p span{
	height:36.6%; color:#ea6243; background:#f7f7f7; display:block; position:absolute; text-align:center; padding-top:3.2%; top:0; left:58%;
}
form p span#sj{
	width:36.25%; font-size:12px; border-radius:0 0 5px 5px;
}
form p span#mi{
	width:40%; font-size:12px; border-radius:0 0 5px 5px;
}
form a{
	color:#43a4db; font-size:14px; position:absolute; right:6%; bottom:0;
}
.dlform .dlk{
	width:93.75%; height:24%; background:#e8a005; display:block; position:absolute; left:3.1%; top:62%; text-align:center;
}
.dlform #dl{
	width:100%; height:100%; background:none; border:none;color:#fff; font-size:18px; font-family:"微软雅黑";
}
.log-mask{
	width: 100%;
	height:100%;
	position: fixed;
	background: rgba(78,78,78,0.5);
	top: 0;
	left: 0;
	display: none;
}
.log-mask>p{
	position: fixed;
	width: 100%;
	text-align: center;
	color: rgba(255,255,255,0.7);
	font-size: 16px;
	top: 55%;
}
.warn{
	width:80%;
	height: 150px;
	position: absolute;
	left: 10%;
	top:50%;
	margin-top: -75px;
	background: rgb(255,255,255);
	display: none;
}
.wanr-text{
	margin-top: 40px;
	color: #7a7a7a;
	text-align: center;
	padding: 0 10px;
	font-size: 12px;
}
.wanr-text>span{
	color: orange;
}
.buttons{
	width: 100%;
	height: 40px;
	position: absolute;
	bottom: 0;
}
.buttons>button{
	width:50%;
	height: 40px;
	float:left;
	background: #efefef;
	color: #666;
	border: none;
	font-family: Microsoft Yahei;
	font-size: 14px;
}
.buttons>button:nth-child(2){
	float:right;
	background: none;
	border: 1px solid #efefef;
	color: skyblue;
}
.loader
 {
  font-size: 60px;
  text-indent: -9999em;
  overflow: hidden;
  width: 1em;
  height: 1em;
  border-radius: 50%;
  margin: 85% auto;
  position: relative;
  animation: load6 1s infinite ease;
  animation: load6 1s infinite ease;
}
@keyframes load6 {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.11em -0.83em 0 
-0.42em #ffffff, -0.11em -0.83em 0 -0.44em #ffffff, -0.11em -0.83em 0 
-0.46em #ffffff, -0.11em -0.83em 0 -0.477em #ffffff;
  }
  5%,
  95% {
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.11em -0.83em 0 
-0.42em #ffffff, -0.11em -0.83em 0 -0.44em #ffffff, -0.11em -0.83em 0 
-0.46em #ffffff, -0.11em -0.83em 0 -0.477em #ffffff;
  }
  30% {
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.51em -0.66em 0 
-0.42em #ffffff, -0.75em -0.36em 0 -0.44em #ffffff, -0.83em -0.03em 0 
-0.46em #ffffff, -0.81em 0.21em 0 -0.477em #ffffff;
  }
  55% {
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.29em -0.78em 0 
-0.42em #ffffff, -0.43em -0.72em 0 -0.44em #ffffff, -0.52em -0.65em 0 
-0.46em #ffffff, -0.57em -0.61em 0 -0.477em #ffffff;
  }
  100% {
    transform: rotate(360deg);
    transform: rotate(360deg);
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.11em -0.83em 0 
-0.42em #ffffff, -0.11em -0.83em 0 -0.44em #ffffff, -0.11em -0.83em 0 
-0.46em #ffffff, -0.11em -0.83em 0 -0.477em #ffffff;
  }
}
@keyframes load6 {
  0% {
    transform: rotate(0deg);
    transform: rotate(0deg);
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.11em -0.83em 0 
-0.42em #ffffff, -0.11em -0.83em 0 -0.44em #ffffff, -0.11em -0.83em 0 
-0.46em #ffffff, -0.11em -0.83em 0 -0.477em #ffffff;
  }
  5%,
  95% {
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.11em -0.83em 0 
-0.42em #ffffff, -0.11em -0.83em 0 -0.44em #ffffff, -0.11em -0.83em 0 
-0.46em #ffffff, -0.11em -0.83em 0 -0.477em #ffffff;
  }
  30% {
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.51em -0.66em 0 
-0.42em #ffffff, -0.75em -0.36em 0 -0.44em #ffffff, -0.83em -0.03em 0 
-0.46em #ffffff, -0.81em 0.21em 0 -0.477em #ffffff;
  }
  55% {
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.29em -0.78em 0 
-0.42em #ffffff, -0.43em -0.72em 0 -0.44em #ffffff, -0.52em -0.65em 0 
-0.46em #ffffff, -0.57em -0.61em 0 -0.477em #ffffff;
  }
  100% {
    transform: rotate(360deg);
    transform: rotate(360deg);
    box-shadow: -0.11em -0.83em 0 -0.4em #ffffff, -0.11em -0.83em 0 
-0.42em #ffffff, -0.11em -0.83em 0 -0.44em #ffffff, -0.11em -0.83em 0 
-0.46em #ffffff, -0.11em -0.83em 0 -0.477em #ffffff;
  }
}

/*---------------注册页样式--------------------*/
form p input{
	font-size:15px;
}
.zcform{
	width:100%; height:52%; position:absolute; top:25.6%;
}
.zcform p{
	width:100%; height:17.8%; background:#fcfcfc; border-top:1px solid #ccc; line-height:50%; position:relative; font-size:14px;
