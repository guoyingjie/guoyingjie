.lottery_info{
	width: 90%;
	height: 100px;
	margin: 0 auto;
}
.loy_logo{
	margin-top: 10px;
	width: 80px;
	height: 80px;
	display: inline-block;
}
.info_txt{
	width: 50%;
	height: 60px;
	line-height: 30px;
	float: right;
	text-align: right;
	margin-top: 20px;
}
.sel_tabel{
	width: 70%;
	height: 30px;
	margin: 10px auto;
	border-bottom: 2px solid #1a9bd2;
	line-height: 30px;
}
.sel_tabel > p{
	width: 70%;
	margin: 0 auto;
	height: 100%;
}
.sel_tabel > p span{
	width: 45%;
	height: 100%;
	text-align: center;
	display: inline-block;
	color: #1a9bd2;
	padding: 0 5px;
}
.sel_tabel > p span.on{
	background: #1a9bd2;
	color: #fff;
}
.sel_tabel > p span:nth-child(2){
	float: right;
}
.red_round,
.my_num,
.blue_round{
	text-align: center;
	width: 90%;
	margin: 10px auto;
}
.blue_round > p,
.my_num > p,
.red_round > p{
	text-align: left;
	font-weight: bold;
	margin-bottom: 10px;
	color: #666;
}
.b i{
	color: #00aaef;
	font-weight: bold;
}
.r i{
	color: #f77074;
	font-weight: bold;
}
.blue_round{
	margin-top: 20px;
}
.my_num{
	height: 71px;
}
.my_num.on{
	margin: 120px auto;
}
.my_num > span{
	position: relative;
}
.my_num > span > i{
	width: 16px;
	height: 16px;
	border-radius: 16px;
	border: 1px solid #f77074;
	color: #f77074;
	text-align: center;
	line-height: 12px;
	display: inline-block;
	position: absolute;
	top: -8px;
	right: -8px;
}
.suer{
	width: 80%;
	height: 40px;
	display: block;
	margin: 20px auto;
	background: #1a9bd2;
	color: #fff;
	border: none;
	border-radius: 6px;
}
.change{
	width: 80%;
	height: 40px;
	display: block;
	margin: 20px auto;
	color: #1a9bd2;
	background: none;
	border: 1px solid #1a9bd2;
	border-radius: 6px;
	display: none;
}