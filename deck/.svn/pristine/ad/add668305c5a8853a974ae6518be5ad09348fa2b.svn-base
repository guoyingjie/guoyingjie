header{
	width: 100%;
	height: 48px;
	background: #47a4dc;
	text-align: center;
	line-height: 48px;
	color: #fff;
	position: relative;
}
.goBack{
	width: 30px;
	height: 48px;
	position: absolute;
	display: inline-block;
	background: url(../../img/newimg/lt.png) no-repeat 10px center;
	background-size: 12px;
	top: 0;
	left: 0;
}
.round{
	width: 100px;
	height: 100px;
	background: #47a4dc url(../../img/newimg/f.png) no-repeat center;
	border-radius: 50%;
	margin-top: 50px;
	display: block;
}
h3{
	text-align: center;
	color: #4c4c4c;
	font-size: 1.1em;
	margin-top: 50px;
	font-weight: normal;
}
.dj{
	text-align: center;
	color: #b2b2b2;
	font-size: .8em;
}
.dj > span{
	font-size: 1.5em !important;
}
.link{
	text-align: center;
	margin-top: 40px;
}
a{
	color: #47a4dc;
}