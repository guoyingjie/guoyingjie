@charset "utf-8";
/* CSS Document */
/*----------------------------公用css------------------------------*/
*{
	padding:0; margin:0 auto;
}
img {
	border:0;
}
ul li {
	list-style:none;
}
a,a:visited {
	text-decoration:none; color: #000;
}
body {
	font-family:"微软雅黑"; font-size:12px; color:#000;
}
.bold {
	font-weight:bold;
}
p,h1,h2,h3,h4,h5,h6,ul,li { 
	display:block;
}
