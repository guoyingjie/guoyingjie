package com.broadeast.util;

import java.math.BigDecimal;
import java.util.List;

public class PagingInfoHelper {
	/**
	 * 依赖ska
	 */

		private int pageNum;// 当前页码数
		private long pageSize;//每页条数
		private int totalNum;//总记录数
		private long totalpageNum;//总页数
		private List<?> list;//结果集
		
		
		public long getTotalpageNum() {
			return totalpageNum;
		}

		public void setTotalpageNum(long totalpageNum) {
			this.totalpageNum = totalpageNum;
		}

		public int getPageNum() {
			return pageNum;
		}

		public void setPageNum(int pageNum) {
			this.pageNum = pageNum;
		}

		public int getTotalNum() {
			return totalNum;
		}

		public void setTotalNum(BigDecimal totalNum) {
			this.totalNum = totalNum.intValue();
		}

		public void setTotalNum(int totalNum) {
			this.totalNum = totalNum;
		}

		public long getPageSize() {
			return pageSize;
		}

		public void setPageSize(long pageSize) {
			this.pageSize = pageSize;
		}

		public long getPagetotalNum() {
			return totalpageNum;
		}

		public void setPagetotalNum(long pagetotalNum) {
			this.totalpageNum = pagetotalNum;
		}

		public List<?> getList() {
			return list;
		}

		public void setList(List<?> list) {
			this.list = list;
		}

		
		
		


}
